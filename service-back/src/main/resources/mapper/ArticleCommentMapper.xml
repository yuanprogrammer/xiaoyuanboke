<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.xiaoyuan.back.mapper.ArticleCommentMapper">

    <delete id="removeByArticleId" parameterType="long">
        delete from xy_article_comment
        where article_id = #{articleId}
    </delete>
</mapper>
